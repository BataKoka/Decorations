{% extends 'base.html.twig' %}

{% block title %}PrintType{% endblock %}

{% block body %}
    <h1>PrintType</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ print_type.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ print_type.name }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ print_type.isActive ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('print_type_index') }}">back to list</a>

    <a href="{{ path('print_type_edit', {'id': print_type.id}) }}">edit</a>

    {{ include('print_type/_delete_form.html.twig') }}
{% endblock %}